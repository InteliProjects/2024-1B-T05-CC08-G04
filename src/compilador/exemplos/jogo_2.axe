        se (tecla = 4){
            mover(circulo, -10, 0);         
        }
        colisao_detectada: colidiu(circulo, quadrado);
        se(colisao_detectada) {
            mostrar("colis√£o detectada");
        } senao {
            mostrar("");
        }

        esperar(150);
}
}